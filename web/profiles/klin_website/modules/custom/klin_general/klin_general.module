<?php

use \Drupal\Core\Form\FormStateInterface;

function klin_general_form_node_form_alter(&$form, FormStateInterface $form_state, $form_id)
{
    if ($form_id !== 'node_customer_edit_form') {
        $form['title'] = [
            '#type' => 'hidden',
            '#value' => hash('md5', time())
        ];

    }
}
